{
  "version": 2,
  "projects": {
    "molecular-atoms-dark-mode-toggler": "libs/molecular/atoms/dark-mode-toggler",
    "molecular-atoms-icon": "libs/molecular/atoms/icon",
    "molecular-atoms-icon-text": "libs/molecular/atoms/icon-text",
    "molecular-atoms-image": "libs/molecular/atoms/image",
    "molecular-atoms-link": "libs/molecular/atoms/link",
    "molecular-atoms-scroll-top": "libs/molecular/atoms/scroll-top",
    "thefront-v3": "apps/thefront-v3",
    "thefront-v3-e2e": "apps/thefront-v3-e2e",
    "theme-styles-global": "libs/theme/styles/global",
    "theme-theme-provider": "libs/theme/theme-provider",
    "ui-layout": "libs/ui/layout",
    "www": {
      "root": "apps/www",
      "sourceRoot": "apps/www",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/www",
            "outputPath": "dist/apps/www"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "www:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "www:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "www:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/www"],
          "options": {
            "jestConfig": "apps/www/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/www/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "www-e2e": {
      "root": "apps/www-e2e",
      "sourceRoot": "apps/www-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/www-e2e/cypress.json",
            "devServerTarget": "www:serve",
            "tsConfig": "apps/www-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "www:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/www-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "www"
}
